lab:
  description: {{name}} NetBox topology generated by netreplica/nrx
  notes: Upload {{name}}.cml.yaml to Cisco Modeling Labs to use this topology
  title: {{name}}
  version: 0.2.0
links:
{% for link in links: %}
  - id: l{{ link['id'] }}
    n1: n{{ link['a']['device_index'] }}
    n2: n{{ link['b']['device_index'] }}
    i1: {{ link['a']['e_interface'] }}
    i2: {{ link['b']['e_interface'] }}
    label: {{ link['a']['node'] }}-{{ link['a']['interface'] }}<->{{ link['b']['node'] }}-{{ link['b']['interface'] }}
{% endfor %}
nodes:
{% for node in rendered_nodes: %}
{{ node }}
{% endfor %}
annotations: []

